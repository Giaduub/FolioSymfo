{% extends 'base.html.twig' %}

{% block title %}Hello HomeController!{% endblock %}

{% block body %}

<div class="row">

 {% for project in projects %}
<div class="col-4">
 <article>
     <h2>{{ project.title }}</h2>
     <div class="content">
         <img class="img-fluid" src="{{ project.image }}" alt="">
         <a href="{{project.Github}}" class="btn btn-primary">Github</a>
         <a href="{{project.Weblink}}" class="btn btn-primary">Weblink</a>
         <a href="{{ path('projet_show', {'id': project.id})}}" class="btn btn-primary">Lire la suite</a>
     </div>
 </article>
</div>
 {% endfor %}
 


{% endblock %}